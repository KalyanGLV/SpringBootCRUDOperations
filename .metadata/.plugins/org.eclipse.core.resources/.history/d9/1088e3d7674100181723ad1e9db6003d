package Utils;

import org.springframework.stereotype.Component;

/*
 * kg972t
 * 
 */

@Component
public class Funclib {

	public static String nullCheckAndSet(Object o) {

		return (!Funclib.isNothing(o) ? String.valueOf(o) : "");

	}

	public static boolean isNothing(Object o) {

		return (o == null);
	}

}
